{
  "name": "YinHaoNodeJsDemo",
  "version": "1.0.0",
  "description": " 面试 demo 项目",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start",
    "dev": "egg-bin dev -r egg-ts-helper/register --workers=1",
    "debug": "egg-bin debug -r egg-ts-helper/register --workers=1",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint .",
    "clean": "ets clean"
  },
  "dependencies": {
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.6.0",
    "egg-view-nunjucks": "^2.2.0",
    "exceljs": "^4.3.0",
    "fast-csv": "^4.3.6",
    "lodash": "4.17.15",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.1",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-cors": "^2.2.3",
    "egg-mock": "^3.16.0",
    "egg-router-plus": "^1.3.1",
    "egg-swagger-doc": "^2.3.2",
    "egg-ts-helper": "^1.4.2",
    "tslib": "^1.9.0",
    "tslint": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "type": "commonjs",
  "author": "qingaoti",
  "license": "MIT"
}
